{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-react",
    "tslint-react-a11y"
  ],
  "rules": {
    "quotemark": true,
    "variable-name": false,
    "jsx-wrap-multiline": false,
    "object-literal-sort-keys": false,
    "jsx-alignment": false
  }
}
